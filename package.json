	},
	"dependencies": {
		"@silvia-odwyer/photon-node": "^0.3.3",
		"@squaresapp/rawjs": "^1.3.14",
		"fila-node": "^1.0.5",
		"linkedom": "^0.16.6"
	},
	"devDependencies": {
		"@types/node": "^20.10.2",
		"moduless": "^3.2.5"
	}
}
