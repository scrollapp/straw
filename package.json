{
	"name": "strawjs",
	"author": "Paul Gordon",
	"description": "A static site generator for those who can't get enough of RawJS",
	"version": "1.2.1",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/squaresapp/straw"
	},
	"bugs": {
		"url": "https://github.com/squaresapp/straw/issues"
	},
	"main": "straw.js",
	"browser": "straw.js",
	"types": "straw.d.ts",
	"bin": "straw.js",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"readme.md",
		"LICENSE.txt",
		"straw.js",
		"straw.d.ts",
		"straw.d.ts.map",
		"straw.min.js"
	],
	"scripts": {
		"test": "exit 0",
		"bundle": "tsc -p tsconfig.release.json",
